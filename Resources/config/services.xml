<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ferrandini_twig_extension.ferrandini_extension.class">Ferrandini\Bundle\TwigExtensionBundle\Twig\FerrandiniExtension</parameter>
    </parameters>

    <services>
        <service id="ferrandini_twig_extension.ferrandini_extension" class="%ferrandini_twig_extension.ferrandini_extension.class%">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
